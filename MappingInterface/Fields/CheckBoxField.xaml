<UserControl x:Class="MappingFramework.MappingInterface.CheckBoxField"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MappingFramework.MappingInterface"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="800">
    <Grid>
        <Label x:Name="ComponentLabel"></Label>
        <CheckBox x:Name="ComponentCheckBox" Margin="200,5,5,5">Checked</CheckBox>
    </Grid>
</UserControl>
